package com.demonew;

public class Penguin {
	public Penguin(String name, int age) { 
        super(); 
    } 
	
	 public void eat(){ 
		 System.out.println(name+"正在吃"); 
	 }
	 public void sleep(){
	      System.out.println(name+"正在睡");
	 }
	 public void introduction() { 
	     System.out.println("大家好！我是" + age + "号" + name + "."); 
	 } 

}
