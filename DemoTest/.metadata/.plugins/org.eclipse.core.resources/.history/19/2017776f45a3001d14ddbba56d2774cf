package com.demonew;

public class Animal {
	private String name;
	private int age;
	private String species;
	

	public Animal () {
		this.setName(name);
		this.setAge(age);
		this.setSpecies(species);
	}
	public void eat() {
		System.out.println("eating");
	}

	public String getSpecies() {
		return species;
	}

	public void setSpecies(String species) {
		this.species = species;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	public void printInfo() {
		System.out.println("nm="+name+"age="+age+"sp="+species);
	}
}
